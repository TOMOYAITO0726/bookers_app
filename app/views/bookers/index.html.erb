<h1>BOOKS</h1>

<%@bookers.each do |booker|%>

 <p>タイトル</p> 
 <span><%=booker.title%></span>
<%end%>

<h1>New book</h1>

<%= form_with model: @booker,url: '/bookers',method: :post  do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Book' %>
<% end %>
