<h1>Editing Book</h1>

<%=form_with model: @booker do |f|%>

<h4>Title</h4>
 <%= f.text_field :title %>
<h4>Body</h4> 
   <%= f.text_area :body %>
   <%=f.submit'Update Book'%>
   
   <span>
<%=link_to "Show","bookers/edit" %>  
</span>

<span>
 <%=link_to "Back","bookers/new" %>  
</span>
   <%end%>
   
   